<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="micrite-crm" extends="struts-default">
		<action name="manage" class="crmAction"
			method="manage">
			<result name="success">/crm/manage.jsp</result>
		</action>
 		<action name="disabled" class="crmAction"
			method="disabled">
			<result name="success">/crm/manage.jsp</result>
		</action>
    </package>

</struts>
